{
  "name": "@minecraft-docker/mod-source-modrinth",
  "version": "1.0.0",
  "description": "Modrinth adapter for minecraft-docker mod source system",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@minecraft-docker/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "minecraft",
    "modrinth",
    "mods",
    "plugins"
  ],
  "license": "MIT"
}
